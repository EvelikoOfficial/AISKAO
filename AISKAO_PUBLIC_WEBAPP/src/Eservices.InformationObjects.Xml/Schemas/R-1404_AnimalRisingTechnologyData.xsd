                <xsd:sequence>
                  <xsd:element name="AnimalRisingTechnologyCode" type="artc:AnimalRisingTechnologyCode" />
                  <xsd:element name="AnimalRisingTechnologyName" type="artn:AnimalRisingTechnologyName" />
