<?xml version="1.0" encoding="utf-8"?>
<xsd:schema targetNamespace="http://ereg.egov.bg/nomenclature/R-1275" xmlns="http://ereg.egov.bg/nomenclature/R-1275" xmlns:xsd="http://www.w3.org/2001/XMLSchema" elementFormDefault="qualified">
  <xsd:simpleType name="AnimalTypeFedWithRelatedAnimalProductsNomenlcature">
